{
   "name": "products",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "add_migration": "cd ./src/migrations && typeorm-ts-node-commonjs migration:generate -d ../data-source.ts",
      "run_migration": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start --watch --debug --exec 'node --inspect=0.0.0.0'",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "dependencies": {
      "@elastic/elasticsearch": "^8.6.0",
      "@google-cloud/translate": "^7.1.0",
      "@nestjs/common": "^9.0.0",
      "@nestjs/core": "^9.0.0",
      "@nestjs/event-emitter": "^1.3.1",
      "@nestjs/platform-express": "^9.0.0",
      "@nestjs/typeorm": "^9.0.1",
      "@types/pg": "^8.6.6",
      "aws-sdk": "^2.1298.0",
      "axios": "^1.2.3",
      "axios-cookiejar-support": "^4.0.6",
      "cheerio": "^1.0.0-rc.12",
      "csv": "^6.2.5",
      "extract-json-from-string": "^1.0.1",
      "fflate": "^0.7.4",
      "gunzip-file": "^0.1.1",
      "natural": "^6.2.0",
      "neo4j-driver": "^5.4.0",
      "openai": "^3.2.0",
      "pg": "^8.4.0",
      "pg-copy-streams": "^6.0.4",
      "progress-promise": "^0.0.6",
      "promise-limit": "^2.7.0",
      "puppeteer": "^19.5.2",
      "qdrant": "^1.3.1",
      "reflect-metadata": "^0.1.13",
      "request-promise": "^4.2.6",
      "rxjs": "^7.2.0",
      "semaphore": "^1.1.0",
      "tough-cookie": "^4.1.2",
      "ts-worker": "^1.0.3",
      "typeorm": "0.3.11",
      "uuid": "^8.3.2",
      "xml2js": "^0.4.23"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.0.0",
      "@nestjs/schematics": "^9.0.0",
      "@nestjs/testing": "^9.0.0",
      "@types/express": "^4.17.13",
      "@types/hapi__joi": "^17.1.9",
      "@types/jest": "29.2.4",
      "@types/node": "^16.11.10",
      "@types/pg-copy-streams": "^1.2.1",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "geojson": "^0.5.0",
      "jest": "29.3.1",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "29.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "4.1.1",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
